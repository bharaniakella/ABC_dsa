      "source": [
        "def largest(arr):\n",
        "  max_element=0\n",
        "  for i in range(len(arr)):\n",
        "    if(arr[i]>arr[max_element]):\n",
        "      max_element=i\n",
        "  return arr[max_element]\n"
      ],
